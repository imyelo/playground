{
  "name": "requirement-parser",
  "version": "1.0.0",
  "main": "lib",
  "license": "MIT",
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "ava": "^3.15.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "tsc -p ./tsconfig.build.json",
    "prebuild": "yarn clean",
    "start": "yarn build -w",
    "test": "ava -v"
  },
  "ava": {
    "require": [
      "ts-node/register"
    ],
    "extensions": [
      "ts"
    ],
    "files": [
      "test/**/*"
    ]
  },
  "dependencies": {
    "rimraf": "^3.0.2"
  }
}
